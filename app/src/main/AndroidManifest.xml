        <activity
            android:name=".Detailmov"
            android:exported="true" />
        <activity
            android:name=".Accountact"
            android:exported="false" />
        <activity
            android:name=".Search"
            android:exported="false" />
        <activity
            android:name=".Hero"
            android:exported="true" />
        <activity
            android:name=".Form_edit"
            android:exported="true" />
        <activity
            android:name=".Item_movie"
            android:exported="true" />
        <activity
            android:name=".Admincrud"
            android:exported="true" />
        <activity
            android:name=".ComingSoon"
            android:exported="true" />
        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>